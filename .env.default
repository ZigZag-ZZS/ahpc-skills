REACT_APP_GEMINI_API_KEY=your_api_key_here

# Database Type Selection
# Possible values: json, mariadb, mysql, postgresql
DB_TYPE=json

# JSON Database Configuration (when DB_TYPE=json)
JSON_DB_PATH=./data/db.json
JSON_DB_BACKUP_ENABLED=true
JSON_DB_BACKUP_INTERVAL=3600000

# MariaDB/MySQL Configuration (when DB_TYPE=mariadb or mysql)
DB_HOST=localhost
DB_USER=root
DB_PASSWORD=your_password
DB_NAME=your_database_name
DB_PORT=3306

# PostgreSQL Configuration (when DB_TYPE=postgresql)
PG_HOST=localhost
PG_USER=postgres
PG_PASSWORD=your_password
PG_DATABASE=your_database_name
PG_PORT=5432