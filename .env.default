REACT_APP_GEMINI_API_KEY=your_api_key_here

# Тип хранилища: 'json' или 'database'
REACT_APP_STORAGE_TYPE=json

# URL API для бекенда (для database режима)
REACT_APP_API_URL=http://localhost:3001/api

# Настройки для базы данных (используются бекендом)
REACT_APP_DB_HOST=localhost
REACT_APP_DB_PORT=5432
REACT_APP_DB_NAME=skills_assessment
REACT_APP_DB_USER=postgres
REACT_APP_DB_PASSWORD=password
REACT_APP_DB_DIALECT=postgres

# Путь для JSON файлов (на сервере)
REACT_APP_JSON_FILE_PATH=./data/testResults.json